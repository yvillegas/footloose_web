<div class="container">
    <div class="row mt-5">
        <h3 class="text-center text-uppercase p-2">Generar Reporte </h3>


        <form action="">
            <div class="card text-center mx-auto">

                <div class="row p-3">
                    <div class="input-group mt-2">
                        <label for="name" class="input-group-text">Rango de Fechas</label>
                        <input class="form-control bg-white" type="date" name="fechaini" id="fechaini"
                            placeholder="Ejemplo: 12-12-21">
                        <input class="form-control bg-white" type="date" name="fechafin" id="fechafin"
                            placeholder="Ejemplo: 12-12-21">
                    </div>
                    <div class="input-group mt-2">
                        <label for="name" class="input-group-text">Rango de Precio</label>
                        <input class="form-control bg-white" type="number" name="precioini" id="precioini" step="0.01"
                            placeholder="10.00">
                        <input class="form-control bg-white" type="number" name="preciofin" id="preciofin" step="0.01"
                            placeholder="20.00">
                    </div>
                    <div class="input-group mt-2">
                        <label for="name" class="input-group-text">Rango de Cantidad</label>
                        <input class="form-control bg-white" type="number" name="cantidadini" id="cantidadini"
                            placeholder="10">
                        <input class="form-control bg-white" type="number" name="cantidadfin" id="cantidadfin"
                            placeholder="20">
                    </div>
                    <div class="input-group mt-2">
                        <label for="name" class="input-group-text">Rango Importe</label>
                        <input class="form-control bg-white" type="number" name="precioini" id="precioini" step="0.01"
                            placeholder="10.00">
                        <input class="form-control bg-white" type="number" name="preciofin" id="preciofin" step="0.01"
                            placeholder="20.00">
                    </div>
                    <div class="form-group mt-4 d-grid gap-2">
                        <button class="btn btn-success"> Descargar</button>
                    </div>

                </div>
            </div>
        </form>


    </div>
</div>